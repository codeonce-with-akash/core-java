package dev.codeonce.mt;

public class DemonstratingJoinMethod implements Runnable {

	public void run() {
		try {
			Thread.sleep(3000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		for (int i = 1; i <= 5; i++) {
			System.out.println("Child Thread - " + i);
		}
	}

}
